{
  "name": "angular2-poke-things-to-learn",
  "version": "1.0.0",
  "scripts": {
    "postinstall": "npm run typings install",
    "tsc": "tsc",
    "tsc:w": "tsc --watch",
    "tsc:exercise": "tsc --watch -p typescript-exercises",
    "live": "live-server --open=/",
    "start": "concurrently \"npm run tsc:w\" \"npm run live\" ",
    "exercise": "concurrently \"npm run tsc:exercise\" \"nodemon --ext ts -w typescript-exercises typescript-exercises/exercise.js\"",
    "build:exercises": "node './.build-exercises.js' *.md",
    "typings": "typings"
  },
  "engines": {
    "node": ">= 6.0.0"
  },
  "enginesStrict": true,
  "license": "AGPL-3",
  "dependencies": {
    "@angular/common": "2.0.0-rc.1",
    "@angular/compiler": "2.0.0-rc.1",
    "@angular/core": "2.0.0-rc.1",
    "@angular/http": "2.0.0-rc.1",
    "@angular/platform-browser": "2.0.0-rc.1",
    "@angular/platform-browser-dynamic": "2.0.0-rc.1",
    "@angular/router": "2.0.0-rc.1",
    "@angular/router-deprecated": "2.0.0-rc.1",
    "@angular/upgrade": "2.0.0-rc.1",
    "core-js": "^2.4.0",
    "foundation-sites": "^6.2.3",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.6",
    "systemjs": "0.19.27",
    "zone.js": "^0.6.12"
  },
  "devDependencies": {
    "concurrently": "^2.0.0",
    "lite-server": "^2.2.0",
    "marked": "^0.3.5",
    "typescript": "^1.8.10",
    "typings": "^1.0.4"
  }
}
